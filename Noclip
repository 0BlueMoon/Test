local Library = loadstring(Game:HttpGet("https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/wizard"))()

local PhantomForcesWindow = Library:NewWindow("Script")

local KillingCheats = PhantomForcesWindow:NewSection("Script")

getgenv().noclip = nil
BlueMoon:CreateToggle("Noclip", function(value)
getgenv().noclip = value
if getgenv().noclip == true and task.wait() then
local Noclip = nil
local Clip = nil
function noclip()
    Clip = false
    local function Noclip()
        if Clip == false and game.Players.LocalPlayer.Character ~= nil then
            for i,v in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
                if v:IsA('BasePart') and v.CanCollide and v.Name ~= floatName then
                    v.CanCollide = false
                end
            end
        end
        wait(0)
    end
    Noclip = game:GetService('RunService').Stepped:Connect(Noclip)
end
function clip()
    if Noclip then Noclip:Disconnect() end
    Clip = true
end
noclip()
else
local Noclip = nil
local Clip = nil
function noclip()
    Clip = false
    local function Noclip()
        if Clip == false and game.Players.LocalPlayer.Character ~= nil then
            for i,v in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
                if v:IsA('BasePart') and v.CanCollide and v.Name ~= floatName then
                    v.CanCollide = true
                end
            end
        end
        wait(0)
    end
    Noclip = game:GetService('RunService').Stepped:Connect(Noclip)
end
function clip()
    if Noclip then Noclip:Disconnect() end
    Clip = true
end
noclip()
end
end)
